{
    "name": "__MSG_appName__",
    "version": "0.0.5",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        // "page": "background.html"
        "scripts":["scripts/chromereload.js","bower_components/jquery/dist/jquery.js", "bower_components/uri.js/src/URI.min.js", "bower_components/lodash/dist/lodash.compat.js", "bower_components/angular/angular.js", "bower_components/angular-local-storage/dist/angular-local-storage.js", "bower_components/bootstrap/dist/js/bootstrap.js", "bower_components/angular-socket-io/socket.js", "bower_components/socket.io-client/socket.io.js", "scripts/background/background.controller.js",  "scripts/messenger.js", "scripts/background/background.socket.js"]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "assist",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "omnibox": {
        "keyword": "assist"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "background",
        "bookmarks",
        "history",
        "http://*/*",
        "https://*/*"
    ]
}